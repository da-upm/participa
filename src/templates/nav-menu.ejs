<% const currentPage=typeof page !=='undefined' ? page : '' ; %>
<ul class="navbar-nav me-auto mb-2 mb-lg-0">
    <li class="nav-item">
        <a class="nav-link <%= (currentPage === 'index') ? 'active' : '' %>"
            aria-current="<%= (currentPage === 'index') ? 'page' : '' %>" href="/">Inicio</a>
    </li>
    <% if (featureFlags.process) { %>
        <li class="nav-item">
            <a class="nav-link <%= (currentPage === 'process') ? 'active' : '' %>"
                aria-current="<%= (currentPage === 'process') ? 'page' : '' %>"
                href="/process">Proceso
            </a>
        </li>
    <% } %>
    <% if (featureFlags.timeline) { %>
        <li class="nav-item">
            <a class="nav-link <%= (currentPage === 'timeline') ? 'active' : '' %>"
                aria-current="<%= (currentPage === 'timeline') ? 'page' : '' %>" href="/timeline">Fechas
            </a>
        </li>
    <% } %>
    <% if (featureFlags.candidates) { %>
        <li class="nav-item">
            <a class="nav-link <%= (currentPage === 'candidates') ? 'active' : '' %>"
                aria-current="<%= (currentPage === 'candidates') ? 'page' : '' %>"
                href="/candidates">Candidaturas
            </a>
        </li>
    <% } %>
    <% if (featureFlags.questions) { %>
        <li class="nav-item">
            <a class="nav-link <%= (currentPage === 'questions') ? 'active' : '' %>"
                aria-current="<%= (currentPage === 'questions') ? 'page' : '' %>"
                href="/questions">Preguntas</a>
        </li>
    <% } %>
    <% if (featureFlags.results) { %>
        <li class="nav-item">
            <a class="nav-link <%= (currentPage === 'results') ? 'active' : '' %>"
                aria-current="<%= (currentPage === 'results') ? 'page' : '' %>"
                href="/results">Escrutinio</a>
        </li>
    <% } %>
</ul>